<header class="belsu-header">
	<div class="belsu-header-wrap">
		<div class="belsu-header-item">
			<div class="belsu-navbar _header _dark">
				<div class="belsu-navbar-wrap">
					<div class="belsu-navbar-item">
						<div class="belsu-logo _header _dark">
							<div class="belsu-logo-wrap">
								<div class="belsu-logo-item">
									<a href="{{ site.url }}"><img
												src="{{ theme.uri }}/assets/image/components/logo/01/logo-dark.png"
												alt="belsuono"></a>
								</div>
								
							</div>
						</div>
					</div>
					<div class="belsu-navbar-item">
						<nav id="menu" class="belsu-menu _header _dark">
							<div id="ul" class="belsu-menu-wrap">
								{% for i in menu_primary.items %}
									<div class="belsu-menu-item {{ i.classes|join(' ') }}">
										<a href="{{ i.link }}">{{ i.title }}</a>
									</div>
								{% endfor %}
							</div>
						</nav>
						<div id="burger" class="bel-burger">
							<img src="{{ theme.uri }}/assets/image/burger.png" alt="">
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="belsu-header-item">
			<div class="belsu-navbar _header _dark">
				<div class="belsu-navbar-wrap">
					<div class="belsu-navbar-item">
						<div class="belsu-logo _header _dark">
							<div class="belsu-logo-wrap">
								<div class="belsu-logo-item">
									<a href="{{ site.url }}">
										<img src="{{ theme.uri }}/assets/image/components/logo/02/logo-light.png"
										     alt="belsuono">
									</a>
								</div>
								
							</div>
						</div>
					</div>
					<div class="belsu-navbar-item">
						<nav class="belsu-menu _header _light">
							<div class="belsu-menu-wrap">
								{% for i in menu_primary.items %}
									<div class="belsu-menu-item {{ i.classes|join(' ') }}">
										<a href="{{ i.link }}">{{ i.title }}</a>
									</div>
								{% endfor %}
							</div>
						</nav>
						<div class="bel-burger"><img src="{{ theme.uri }}/assets/image/burger-light.png" alt="">
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</header>
